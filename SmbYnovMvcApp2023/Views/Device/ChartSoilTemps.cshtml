@model DataToDisplay

@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Temp sol")
        .AddSeries(
            name: "Temp sol",
            chartType: "Line",
            yValues: new[] { Model.valueTempSoil },
            xValues: new[] { Model.dateTempSoil })
        .Write();
}