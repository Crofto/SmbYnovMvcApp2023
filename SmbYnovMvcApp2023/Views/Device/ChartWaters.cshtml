@model DataToDisplay

@{
    var myChart = new  System.Web.Helpers.Chart(width: 600, height: 400)
        .AddTitle("Humidite")
        .AddSeries(
            name: "Humidite",
            chartType: "Line",
            yValues: new[] { Model.valueWater },
            xValue: new[] { Model.dateWater })
        .Write();
}